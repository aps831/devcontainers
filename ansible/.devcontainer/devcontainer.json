{
	"name": "Ansible",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"PYTHON_VERSION": "3.9-buster",			
			"NODE_VERSION": "lts/*",
			"GITLEAKS_VERSION": "8.5.1"
		}
	},	
	"features": {
		"github-cli": "latest"
	},
	"settings": {
		"files.associations": {
			"**/*.yaml": "ansible",
			"**/*.yml": "ansible"
		}
	}
}
